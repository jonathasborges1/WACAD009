{
   "parser": "@typescript-eslint/parser",
   "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
   ],
   "parserOptions" : {
      "ecmaVersion": "latest",
      "sourceType": "module",
   },
   "rules": {
      "@typescript-eslint/explicit-function-return-type":0,
      "@typescript-eslint/explicit-module-boundary-types":0
   }
}